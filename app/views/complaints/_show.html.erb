<div class="modal fade" id="showModal" tabindex="-1" role="dialog" aria-labelledby="showModal" aria-hidden="true">
  <div class="modal-dialog modal-dialog-centered" role="document">
    <div class="modal-content">
      <div class="create_form">
        <h5 class="modal-title" id="exampleModalLongTitle">Complaint details</h5>
        <p class="modal-address" id="exampleModaladdress"> <%= complaint.address %></p>
        <p class="modal-address" id="exampleModalCategory"> <%= complaint.category %></p>
        <p class="modal-address" id="exampleModalDescription"> <%= complaint.description %></p>
        <p class="modal-address" id="exampleModalUser"> <%= complaint.user %></p>
        <button type="button" class="close" data-dismiss="modal" aria-label="Close">
          <span aria-hidden="true">&times;</span>
        </button>
      </div>
      <div class="modal-body">
      </div>
    </div>
  </div>
</div>
